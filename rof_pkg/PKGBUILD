pkgname=('rof-git')
pkgver=0.0.1
pkgrel=1
pkgdesc="Script to launch the command given as an argument if not yet running, else set focus to appropriate window"
arch=('all')
url="https://github.com/nwg-piotr/tint2-executors"
license=('GPL3')
depends=('xorg-xprop' 'wmctrl')
source=("https://github.com/nwg-piotr/obhud/raw/master/dist/obhud-0.2.1.tar.gz")

md5sums=('4f55d17214ca1da63649df51f392e166')

package() {
  install -D -m 755 rof \
 	 "$pkgdir"/usr/bin/rof
}